{
  "name": "character-creation-platform",
  "version": "1.0.0",
  "description": "AI-powered character creation platform",
  "main": "dist/apps/server/src/index.js",
  "scripts": {
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "tsc -p tsconfig.client.json",
    "dev": "npm run dev:server",
    "dev:server": "ts-node -P tsconfig.server.json apps/server/src/index.ts",
    "dev:client": "tsc -p tsconfig.client.json --watch",
    "start": "node dist/apps/server/src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "type-check:server": "tsc -p tsconfig.server.json --noEmit",
    "type-check:client": "tsc -p tsconfig.client.json --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "db:setup": "ts-node src/scripts/setup-db.ts",
    "db:test": "ts-node src/scripts/test-db.ts"
  },
  "keywords": [
    "character",
    "creation",
    "ai",
    "platform"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.478.0",
    "@aws-sdk/client-s3": "^3.478.0",
    "@aws-sdk/s3-request-presigner": "^3.478.0",
    "@google/genai": "^1.17.0",
    "@prisma/client": "^5.7.0",
    "@reduxjs/toolkit": "^2.0.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^6.0.2",
    "@types/pg": "^8.10.7",
    "@types/uuid": "^10.0.0",
    "auth0": "^4.1.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-oauth-server": "^2.0.0",
    "express-openid-connect": "^2.17.1",
    "express-rate-limit": "^8.1.0",
    "helmet": "^7.1.0",
    "https-proxy-agent": "^7.0.6",
    "jsonwebtoken": "^9.0.2",
    "jwks-client": "^2.0.5",
    "node-fetch": "^2.6.12",
    "pg": "^8.11.3",
    "prisma": "^5.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.4",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.8.7",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/sharp": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
